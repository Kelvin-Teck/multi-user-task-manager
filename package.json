{
  "name": "multi-user-taskmanager",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/index.js",
    "dev": "nodemon",
    "build": "tsc",
    "migration:generate": "npx sequelize-cli migration:generate --name",
    "seed:generate": "npx sequelize-cli seed:generate --name ",
    "db:migrate": "npx sequelize-cli db:migrate",
    "undo:db:migrate": "npx sequelize-cli db:migrate:undo --name",
    "undo:db:migrate:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migration": "ts-node --require tsconfig-paths/register --files src/database/migrate",
    "gen-doc": "ts-node swagger/generator",
    "seed": "ts-node --require tsconfig-paths/register --files src/database/seed"
  },
  "keywords": [
    "node",
    "typescript",
    "postgres",
    "sequelize"
  ],
  "author": "Eneh kelvin Chukwuemeka",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.5",
    "@types/nodemailer-express-handlebars": "^4.0.5",
    "@types/pg": "^8.11.10",
    "@types/sequelize": "^4.28.20",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.7",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@sequelize/core": "^7.0.0-alpha.43",
    "@sequelize/postgres": "^7.0.0-alpha.43",
    "@sequelize/validator.js": "^7.0.0-alpha.43",
    "@types/express": "^5.0.0",
    "@types/nodemailer": "^6.4.16",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-handlebars": "^8.0.1",
    "express-rate-limit": "^7.4.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.15",
    "nodemailer-express-handlebars": "^7.0.0",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.4",
    "tsconfig-paths": "^4.2.0",
    "umzug": "^3.8.2",
    "uuid": "^10.0.0"
  }
}
